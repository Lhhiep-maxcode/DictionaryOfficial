<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>

<AnchorPane prefHeight="640.0" prefWidth="960.0" stylesheets="@css/FlashCard.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.dictionaryofficial.FlashCardController">
   <children>
      <Label fx:id="taskbar" layoutX="255.0" layoutY="551.0" prefHeight="70.0" prefWidth="418.0" stylesheets="@css/intoprogram.css" />
      <Button fx:id="setting_button" layoutX="574.0" layoutY="561.0" mnemonicParsing="false" onAction="#showSettingScene" prefHeight="50.0" prefWidth="50.0" stylesheets="@css/button.css" />
      <Button fx:id="edit_button" layoutX="411.0" layoutY="561.0" mnemonicParsing="false" onAction="#addScene" prefHeight="50.0" prefWidth="50.0" stylesheets="@css/button.css" />
      <Button fx:id="game_button" layoutX="492.0" layoutY="561.0" mnemonicParsing="false" onAction="#gameScene" prefHeight="50.0" prefWidth="50.0" stylesheets="@css/button.css" />
      <Button fx:id="search_button" layoutX="317.0" layoutY="564.0" mnemonicParsing="false" onAction="#searchScene" prefHeight="50.0" prefWidth="50.0" stylesheets="@css/button.css" />
      <StackPane fx:id="FlashCardPane" layoutX="143.0" layoutY="41.0" onMouseClicked="#initialize" prefHeight="386.0" prefWidth="674.0">
         <children>
            <Label fx:id="cardLabel" alignment="CENTER" prefHeight="328.0" prefWidth="410.0" />
         </children></StackPane>
      <Button fx:id="prevButton" layoutX="391.0" layoutY="478.0" mnemonicParsing="false" text="Prev" />
      <Button fx:id="nextButton" layoutX="521.0" layoutY="478.0" mnemonicParsing="false" text="Next" />
      <Label fx:id="curLabel" layoutX="459.0" layoutY="481.0" prefHeight="19.0" prefWidth="42.0" text="Label" />
   </children>
</AnchorPane>
