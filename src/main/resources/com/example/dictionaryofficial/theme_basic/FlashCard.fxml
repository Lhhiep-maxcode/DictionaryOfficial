<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="640.0" prefWidth="960.0" stylesheets="@css/FlashCard.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.dictionaryofficial.FlashCardController">
   <children>
      <Label fx:id="taskbar" layoutX="255.0" layoutY="551.0" prefHeight="70.0" prefWidth="493.0" stylesheets="@css/intoprogram.css" />
      <Button fx:id="setting_button" layoutX="656.0" layoutY="561.0" mnemonicParsing="false" onAction="#showSettingScene" prefHeight="50.0" prefWidth="50.0" stylesheets="@css/button.css" />
      <Button fx:id="edit_button" layoutX="411.0" layoutY="561.0" mnemonicParsing="false" onAction="#showAddScene" prefHeight="50.0" prefWidth="50.0" stylesheets="@css/button.css" />
      <Button fx:id="game_button" layoutX="492.0" layoutY="561.0" mnemonicParsing="false" onAction="#showGameScene" prefHeight="50.0" prefWidth="50.0" stylesheets="@css/button.css" />
      <Button fx:id="search_button" layoutX="317.0" layoutY="564.0" mnemonicParsing="false" onAction="#showSearchScene" prefHeight="50.0" prefWidth="50.0" stylesheets="@css/button.css" />
      <StackPane fx:id="FlashCardPane" layoutX="143.0" layoutY="41.0" onMouseClicked="#initialize" prefHeight="386.0" prefWidth="674.0">
         <children>
            <Label fx:id="cardLabel" alignment="CENTER" prefHeight="328.0" prefWidth="410.0" textAlignment="CENTER" wrapText="true" />
         </children></StackPane>
      <Button fx:id="prevButton" layoutX="333.0" layoutY="456.0" mnemonicParsing="false" opacity="0.8" prefHeight="70.0" prefWidth="70.0" rotate="180.0" stylesheets="@css/button.css" />
      <Button fx:id="nextButton" layoutX="564.0" layoutY="456.0" mnemonicParsing="false" opacity="0.8" prefHeight="70.0" prefWidth="70.0" styleClass="root" stylesheets="@css/button.css" />
      <Label fx:id="curLabel" alignment="CENTER" layoutX="417.0" layoutY="475.0" prefHeight="30.0" prefWidth="136.0" text="Label">
         <font>
            <Font name="System Bold" size="22.0" />
         </font></Label>
      <Button fx:id="returnButton" layoutX="11.0" layoutY="14.0" mnemonicParsing="false" onAction="#showGameScene" opacity="0.8" prefHeight="50.0" prefWidth="50.0" stylesheets="@css/button.css" />
      <Button fx:id="translate_button" layoutX="574.0" layoutY="562.0" mnemonicParsing="false" onAction="#showTranslateScene" prefHeight="50.0" prefWidth="50.0" stylesheets="@css/button.css" />
      <Label fx:id="search_label" layoutX="318.0" layoutY="531.0" prefHeight="18.0" prefWidth="60.0" text="Search" textFill="#040505">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="edit_label" layoutX="411.0" layoutY="531.0" prefHeight="27.0" prefWidth="38.0" text="Add" textFill="#040505">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="studying_label" layoutX="480.0" layoutY="530.0" prefHeight="27.0" prefWidth="72.0" text="Studying" textFill="#040505">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="gg_label" layoutX="529.0" layoutY="530.0" prefHeight="27.0" prefWidth="137.0" text="Google Translate" textFill="#040505">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="setting_label" layoutX="656.0" layoutY="531.0" prefHeight="27.0" prefWidth="72.0" text="Setting" textFill="#040505">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="nextGameButton" layoutX="902.0" layoutY="14.0" mnemonicParsing="false" onAction="#PlayBlossomGame" opacity="0.8" prefHeight="50.0" prefWidth="50.0" stylesheets="@../css/button.css" />
   </children>
</AnchorPane>
