<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="640.0" prefWidth="960.0" stylesheets="@css/background.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.dictionaryofficial.SettingController">
    <children>
      <Label fx:id="taskbar" layoutX="255.0" layoutY="551.0" prefHeight="70.0" prefWidth="493.0" stylesheets="@css/intoprogram.css" />
        <Button fx:id="logoutButton" layoutX="891.0" layoutY="602.0" mnemonicParsing="false" onAction="#logout" text="Logout" />
        <Button fx:id="edit_button" layoutX="411.0" layoutY="561.0" mnemonicParsing="false" onAction="#showAddScene" prefHeight="50.0" prefWidth="50.0" stylesheets="@css/button.css" />
        <Button fx:id="game_button" layoutX="492.0" layoutY="561.0" mnemonicParsing="false" onAction="#showGameScene" prefHeight="50.0" prefWidth="50.0" stylesheets="@css/button.css" />
        <Button fx:id="search_button" layoutX="317.0" layoutY="564.0" mnemonicParsing="false" onAction="#showSearchScene" prefHeight="50.0" prefWidth="50.0" stylesheets="@css/button.css" />
      <Pane fx:id="pane" layoutX="47.0" layoutY="22.0" prefHeight="525.0" prefWidth="864.0" stylesheets="@css/setting.css">
         <children>
            <Label alignment="CENTER" layoutX="106.0" layoutY="9.0" prefHeight="37.0" prefWidth="65.0" text="Font" textAlignment="JUSTIFY" textFill="#083c63">
               <font>
                  <Font size="23.0" />
               </font></Label>
            <Label layoutX="113.0" layoutY="63.0" prefHeight="34.0" prefWidth="106.0" text="Font Size" textFill="#083c63">
               <font>
                  <Font size="23.0" />
               </font></Label>
            <ComboBox fx:id="font" layoutX="322.0" layoutY="15.0" prefWidth="150.0" />
            <ComboBox fx:id="fontSize" layoutX="322.0" layoutY="68.0" prefWidth="150.0" />
            <Button fx:id="confirm_button" layoutX="397.0" layoutY="424.0" mnemonicParsing="false" onAction="#buttonAccept" prefHeight="60.0" prefWidth="60.0" stylesheets="@css/button.css">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
            <Label layoutX="116.0" layoutY="116.0" prefHeight="34.0" prefWidth="106.0" text="Theme" textFill="#083c63">
               <font>
                  <Font size="23.0" />
               </font>
            </Label>
            <ComboBox fx:id="theme" layoutX="322.0" layoutY="120.0" prefWidth="150.0" />
         </children>
      </Pane>
        <Button fx:id="setting_button" layoutX="656.0" layoutY="561.0" mnemonicParsing="false" onAction="#buttonAccept" prefHeight="50.0" prefWidth="50.0" stylesheets="@css/button.css" />
      <Button fx:id="translate_button" layoutX="573.0" layoutY="561.0" mnemonicParsing="false" onAction="#showTranslateScene" prefHeight="50.0" prefWidth="50.0" stylesheets="@css/button.css" />

    </children>
</AnchorPane>
